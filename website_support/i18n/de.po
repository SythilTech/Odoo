#. module: website_support
#: model:mail.template,body_html:website_support.support_ticket_reply_wrapper
msgid ""
"\n"
"                ${object.body|safe}\n"
"                <hr/>\n"
"                % if object.ticket_id.portal_access_key :\n"
"                    <b>View Ticket Online:</b> <a href=\"/support/portal/"
"ticket/view/${object.ticket_id.portal_access_key}\">here</a><br/>\n"
"                % endif\n"
"                <b>Ticket Number:</b> ${object.ticket_id.ticket_number or "
"object.ticket_id.id}<br/>\n"
"                <b>Ticket Category:</b> ${object.ticket_id.category.name or "
"''}<br/>\n"
"                % if object.ticket_id.planned_time_format :\n"
"                    <b>Planned Date:</b> ${object.ticket_id."
"planned_time_format}<br/>\n"
"                % endif\n"

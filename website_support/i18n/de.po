"                ${object.ticket_id.description|safe}\n"
"            \n"
"            "
msgstr ""
"\n"
"                ${object.body|safe}\n"
"                <hr/>\n"
"                 % if object.ticket_id.portal_access_key :\n"
"                     <b>Gehe zu Ticket Online:</b> <a href=\"/support/portal/"
"ticket/view/${object.ticket_id.portal_access_key}\">here</a><br/>\n"
"                 % endif\n"

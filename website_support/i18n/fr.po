"                ${object.ticket_id.description|safe}\n"
"            \n"
"            "
msgstr ""

#. module: website_support
#: model:mail.template,body_html:website_support.support_ticket_approval
msgid ""
"\n"
"                % if object.person_name :\n"
"                    <p>Dear ${object.person_name},</p>\n"
"                    <br/>\n"
"                % endif\n"
"\n"
"                <p>Approval is required before we can proceed with this "
"support request, please click the link below to accept</p>\n"
"                <br/>\n"
"                <p><a href=\"${object.approve_url}\" style=\"color:green"
"\">Approve</a> | <a href=\"${object.disapprove_url}\" style=\"color:red"
"\">Don't Approve</a></p>\n"
"\n"
"            \n"
"            "
msgstr ""
"\n"
"                % if object.person_name :\n"
"                    <p>Cher ${object.person_name},</p>\n"
"                    <br/>\n"
"                % endif\n"
"\n"
"                <p>Approbation est nécessaire avant que nous puissions "
"procéder à cette demande de support, s'il vous plaît cliquez sur le lien ci-"
"dessous pour accepter </p>\n"
"                <br/>\n"
"                <p><a href=\"${object.approve_url}\" style=\"color:green"
"\">Approve</a> | <a href=\"${object.disapprove_url}\" style=\"color:red"
"\">Don't Approve</a></p>\n"
"\n"

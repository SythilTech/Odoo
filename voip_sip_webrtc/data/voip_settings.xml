<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <function id="default_inactivity_time"
            model="ir.default" name="set"
            eval="('voip.settings', 'inactivity_time', 10)"/>
    
        <function id="default_ring_duration"
            model="ir.default" name="set"
            eval="('voip.settings', 'ring_duration', 30)"/>

        <function id="default_ringtone_filename"
            model="ir.default" name="set"
            eval="('voip.settings', 'ringtone_filename', 'Old School Ringtone')"/>

        <function id="default_ringtone"
            model="ir.default" name="set"
            eval="('voip.settings', 'ringtone_id', ref('old_school') )"/>
    
    </data>     
</odoo>